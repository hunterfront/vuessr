  "name": "vuessr",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node server",
    "dev:development": "cross-env NODE_ENV=development SERVICE=development node server",
    "dev:test": "cross-env NODE_ENV=development SERVICE=test node server",
    "dev:production": "cross-env NODE_ENV=development SERVICE=production node server",
    "build:development:client": "cross-env NODE_ENV=production SERVICE=development webpack --config build/webpack.client.config.js --progress",
    "build:test:client": "cross-env NODE_ENV=production SERVICE=test webpack --config build/webpack.client.config.js --progress",
    "build:production:client": "cross-env NODE_ENV=production SERVICE=production webpack --config build/webpack.client.config.js --progress",
    "build:development:server": "cross-env NODE_ENV=production SERVICE=development webpack --config build/webpack.server.config.js --progress",
    "build:test:server": "cross-env NODE_ENV=production SERVICE=test webpack --config build/webpack.server.config.js --progress",
    "build:production:server": "cross-env NODE_ENV=production SERVICE=production webpack --config build/webpack.server.config.js --progress",
    "build:development": "rimraf dist && npm run build:development:client && npm run build:development:server",
    "build:test": "rimraf dist && npm run build:test:client && npm run build:test:server",
    "build:production": "rimraf dist && npm run build:production:client && npm run build:production:server"
  },
